//import {TabulatorFull as Tabulator} from 'tabulator-tables';
import {Tabulator} from 'tabulator-tables';
// @import  "tabulator-tables"
import tabledata from '../data/player.json';
// import {Tabulator} from 'tabulator-tables';
//import {TabulatorFull as Tabulator} from 'tabulator-tables';
// import Tabulator from '../scripts/tabulator.min.js'
// const Tabulator = window.Tabulator;
//console.log(tabledata)
const table = new Tabulator("#player-table", {
 	height:500, // set height of table (in CSS or here), this enables the Virtual DOM and improves render speed dramatically (can be any valid css height value)
 	data:tabledata, //assign data to table
 	layout:"fitColumns", //fit columns to width of table (optional)
 	columns:[ //Define Table Columns
	 	{title:"First", field:"firstName", width:100},
	 	{title:"Last", field:"lastName", width:100},
        {title:"Gender", field:"gen"},
	 	{title:"Email", field:"email"},
        {title:"Phone", field:"phone"}
 	],
});

